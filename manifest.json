{
  "name": "Youtube AdBlock",
  "version": "1.0",
  "description": "Extension that blocks all ads on Youtube",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "http://*/",
    "https://*/"
  ],
  "options_page": "pages/options.html",
  "background": {
    "scripts": [
      "scripts/background.js",
      "scripts/blockads.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_icon": {
      "16": "images/disabled_16.png",
      "32": "images/disabled_32.png",
      "48": "images/disabled_48.png",
      "128": "images/disabled_128.png"
    }
  },
  "icons": {
    "16": "images/disabled_16.png",
    "32": "images/disabled_32.png",
    "48": "images/disabled_48.png",
    "128": "images/disabled_128.png"
  },
  "manifest_version": 2
}